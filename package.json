{
  "name": "missririca-client",
  "private": true,
  "version": "1.5.3",
  "type": "module",
  "scripts": {
    "prebuild1": "cd submodules/misskey-v13/packages/misskey-js && pnpm build",
    "prebuild2": "cd submodules/misskey-v13/packages/frontend  && pnpm build",
    "dev": "cp capacitor.config.dev.json capacitor.config.json;cap copy; vite --host;",
    "build": "pnpm prebuild1 && pnpm prebuild2 && cp capacitor.config.prod.json capacitor.config.json ; vite build;cap copy",
    "devbuild": "cp capacitor.config.prod.json capacitor.config.json; vite build  --mode development; cap sync",
    "lint": "vue-tsc",
    "preview": "vite preview",
    "ios": "npx cap open ios",
    "android": "studio android"
  },
  "dependencies": {
    "semantic-ui-css": "^2.5.0",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.0.0",
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/core": "^10.1.2",
    "add": "^2.0.6",
    "misskey-js": "workspace:^",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.8.4",
    "vite": "^4.1.4",
    "vue-tsc": "^1.2.0"
  }
}
